@import "style/weui.wxss";
/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  /* padding: 200rpx 0; */
  box-sizing: border-box;
} 
/* 字体图标 */
@font-face {
  font-family: 'iconfont';  /* project id 1680514 */
  src: url('//at.alicdn.com/t/font_1680514_aw7g1vubhjh.eot');
  src: url('//at.alicdn.com/t/font_1680514_aw7g1vubhjh.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1680514_aw7g1vubhjh.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1680514_aw7g1vubhjh.woff') format('woff'),
  url('//at.alicdn.com/t/font_1680514_aw7g1vubhjh.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1680514_aw7g1vubhjh.svg#iconfont') format('svg');
}
.iconfont{
  font-family:"iconfont" !important;
  font-size:16px;font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
/*本项目使用的图标  */
.icon-go:before{content: '\e606';}
.icon-list:before{content: '\e612';}
.icon-my:before{content:'\e7a3';}
.icon-security:before{content:'\e624';}
.icon-back-01::before {
    content: '\e610';
}
.fl{ float:left; }
.fr{ float:right; }
.clear{ clear:both;}
/* 字体颜色 */
.cgreen{
  color:green;
}
/* 对齐方式 */
.text-center{
  text-align: center;
}
.text-right{
  text-align: right;
}
.text-left{
  text-align: left;
}
.w50{ width:50%; }
.w16{ width:16%; }
.w26{ width:26%; }
.w17{ width:17%; }