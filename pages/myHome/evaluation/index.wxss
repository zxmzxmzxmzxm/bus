/* pages/myHome/evaluation/index.wxss */
page{
  background-color: #f1f1f1;
  font-size: 28rpx;
}
.evaluation-list{
  margin: 40rpx;
}
.textarea{
  width: 100%;
  height: 400rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin: 30rpx 0;
  padding: 20rpx;
  box-sizing: border-box;
}
.linkPhone{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin-top: 30rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.submit{
  width: 100%;
  background-color: #1AA2EE;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  border-radius: 10rpx;
  margin-top: 40rpx;
}